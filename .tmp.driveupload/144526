package com.jspiders.beans;

public class Address {
	private int HouseNo;
	private String City;
	private int PinCode;
	public Address(int houseNo, String city, int pinCode) {
		super();
		HouseNo = houseNo;
		City = city;
		PinCode = pinCode;
	}
	@Override
	public String toString() {
		return "HouseNo: " + HouseNo + ", " + City + ", " + PinCode;
	}
}